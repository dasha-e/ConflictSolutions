#include <iostream>
int main() {
    int n, i;
    std::cout << "Enter the dimension of the array: ";
    std::cin >> n;
    int *a = new int[n];
    std::cout << "Enter the array\n";
    for (i = 0; i < n; i++) {
        std::cout << "a[" << i + 1 << "]: ";
        std::cin >> a[i];
    }
    //Insert Sort
    int key = 0, temp = 0;
	for (int i = 0; i < n - 1; i++)
	{
		key = i + 1;
		temp = a[key];
		for (int j = i + 1; j > 0; j--)
		{
			if (temp < a[j - 1])
			{
				a[j] = a[j - 1];
				key = j - 1;
			}
		}
		a[key] = temp;
	}
    for (i = 0; i < n; i++) {
        std::cout << a[i] << " ";
    }
    std::cout << std::endl;
    return 0;
}
