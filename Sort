#include <iostream>
int main() {
    int n, i;
    std::cout << "Enter the dimension of the array: ";
    std::cin >> n;
    int *a = new int[n];
    std::cout << "Enter the array\n";
    for (i = 0; i < n; i++) {
        std::cout << "a[" << i + 1 << "]: ";
        std::cin >> a[i];
    }
    // bubble sort
    int temp;
    for (int j = 0; j < n; j++) {
        for (i = 1; i < n; i++) {
               if (a[i] < a[i - 1]) {
                temp = a[i];
                a[i] = a[i - 1];
                a[i - 1] = temp;
            }
        }
    }
    for (i = 0; i < n; i++) {
        std::cout << a[i] << " ";
    }
    std::cout << std::endl;
    return 0;
}
